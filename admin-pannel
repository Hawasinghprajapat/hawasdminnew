	<script>

		$(document).ready(function () {
			$(".btn-show").click(function () {
				$(".aside").toggleClass("aside-show")				
				$(".modal").removeClass("show")
			});
		});

		$(document).ready(function () {
			$("#iconSidenav").click(function () {
				$(".aside").removeClass("aside-show")
			});

			$('.fixed-plugin').on('click', function() {
            $('.aside').removeClass("aside-show");
          });

        $('.light-green').on('click', function() {
            $('body').toggleClass('dark-verson');
        });

	   $('.navbar-fix').on('click', function() {
            $('.navbar-main').toggleClass('navbar-sticky');
       });
      
	});
   $(document).ready(function(){
   $('.light') .on ('click')
	
